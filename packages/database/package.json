{
  "name": "@repo/database",
  "version": "1.0.0",
  "main": "dist/src/client.js",
  "module": "dist/src/client.js",
  "types": "dist/src/client.d.ts",
  "exports": {
    ".": {
      "import": "./dist/src/client.js",
      "require": "./dist/src/client.js",
      "types": "./dist/src/client.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/**/*.ts generated/**/*.ts --dts",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "tsx src/seed.ts",
    "dev": "tsup src/**/*.ts generated/**/*.ts --watch",
    "format": "prisma format",
    "generate": "prisma generate",
    "lint": "eslint . --max-warnings 0",
    "prebuild": "npm run generate && node scripts/patch-generated-prisma.js",
    "predev": "npm run generate && node scripts/patch-generated-prisma.js",
    "studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "latest"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^20.19.27",
    "dotenv": "^17.2.3",
    "eslint": "^8.57.0",
    "prisma": "latest",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.2",
    "tsx": "4.19.1",
    "typescript": "5.5.4"
  }
}
